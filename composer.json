{
    "name": "bdx/wn-redirect-conditions-plugin",
    "description": "Allows developers to create their own Redirect Conditions extension plugins for Winter CMS.",
    "license": "GPL-2.0-only",
    "type": "winter-plugin",
    "authors": [
        {
            "name": "Van der Let & Partners",
            "email": "octobercms@bdx.nl",
            "role": "Original Author"
        },
        {
            "name": "Eric Pfeiffer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "composer/installers": "^1.0 || ^2.0"
    },
    "archive": {
        "exclude": [
            ".gitattributes",
            ".github",
            ".gitignore",
            "tests",
            "phpunit.xml"
        ]
    },
    "require-dev": {
        "laravel/pint": "^1.1"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
